{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "itis-embedded-core",
  "author": "Dmitry Kodolbenko",
  "module": "dist/itis-embedded-core.esm.js",
  "size-limit": [
    {
      "path": "dist/itis-embedded-core.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/itis-embedded-core.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.6",
    "husky": "^9.1.6",
    "size-limit": "^11.1.6",
    "tsdx": "^0.14.1",
    "tslib": "^2.8.1",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@ton/ton": "^15.1.0",
    "@twa-dev/sdk": "^7.10.1",
    "ton-crypto": "^3.2.0"
  }
}
